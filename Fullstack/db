create table TRANSFER_TBL(
Transaction_ID Number(4) PRIMARY KEY,
Account_Number varchar2(10) FOREIGN KEY refernce(Account Number),
Beneficiary_account_number varchar2(10) FOREIGN KEY reference (Acount_Number),
Transaction_Date Date,
Transaction_Amount Number(10,2)
);  
