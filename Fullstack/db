Create table Account_Tb(
Account_number varchar2(10) PRIMARY KEY ,
Customer_name varchar2(15),
Balance Number(10,2)
);
Insert into Account_Tb(Account_number,Customer_name,Balance)
Values('1234567890','Reddy',80000);
